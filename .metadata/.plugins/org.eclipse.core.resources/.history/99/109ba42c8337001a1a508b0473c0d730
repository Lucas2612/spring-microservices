package com.rest.webservices.restfulwebservices.user;

public class Post {

	private User user;
	private String text;
	
	public Post(User user, String text) {
		super();
		this.user = user;
		this.text = text;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}
	
	
	
}
